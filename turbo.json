{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "eslint": {
      "dependsOn": [
        "^eslint"
      ],
      "cache": false
    },
    "turbo:build": {
      "dependsOn": [
        "docs#docs:build",
        "docs#docs:test"
      ],
      "outputMode": "new-only"
    },
    "docs#docs:build": {
      "outputMode": "new-only",
      "cache": true,
      "outputs": [
        ".vitepress/dist/**"
      ]
    },
    "docs#docs:test": {
      "outputMode": "new-only",
      "cache": true,
      "docs:test": {
        "cache": true
      },
      "dependsOn": [
        "docs#docs:build"
      ]
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
