{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "//#default": {
      "cache": false
    },
    "compile": {
      "outputs": [
        "dist/**",
        "lib/**"
      ],
      "outputMode": "new-only"
    },
    "test": {
      "dependsOn": [
        "compile"
      ],
      "outputs": [
        "coverage**",
        "test-reports/**"
      ],
      "outputMode": "new-only"
    },
    "package": {
      "dependsOn": [
        "test"
      ],
      "outputMode": "new-only"
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
