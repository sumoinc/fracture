{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "//#default": {
      "cache": false
    },
    "compile": {
      "dependsOn": [
        "^compile"
      ],
      "outputs": [
        "dist/**",
        "lib/**"
      ],
      "outputMode": "new-only"
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [
        "coverage**",
        "test-reports/**"
      ],
      "outputMode": "new-only"
    },
    "synth": {
      "dependsOn": [
        "^synth"
      ],
      "outputs": [
        "cdk-out/**"
      ],
      "outputMode": "new-only"
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
